<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="root" alignment="CENTER" maxWidth="${root.parent.width}" spacing="10.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="textLabel" maxWidth="1.7976931348623157E308" text="Username &gt; Jab text" wrapText="true" HBox.hgrow="ALWAYS" />
    <Button fx:id="likeButton" text="_Like" />
    <Label fx:id="likesLabel" text="0" />
  </children>
  <padding>
    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
  </padding>
</HBox>
