<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="200.0" minWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <TextField fx:id="usernameTextField" promptText="Username" />
    <HBox alignment="CENTER_RIGHT" spacing="20.0">
      <children>
        <Button fx:id="signInButton" text="_Sign in" />
        <Button fx:id="registerButton" text="_Register" />
      </children>
    </HBox>
  </children>
  <padding>
    <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
  </padding>
</VBox>
